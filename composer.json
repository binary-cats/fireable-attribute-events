{
    "name": "binary-cats/fireable-attribute-events",
    "description": "Observe and trigger events based on attributes changes",
    "keywords": [
        "Binary Cats",
        "laravel",
        "eloquent",
        "events",
        "observer"
    ],
    "homepage": "https://github.com/binary-cats",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyrill Kalita",
            "email": "cyrill.kalita@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^2.0||^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "BinaryCats\\FireableAttributeEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinaryCats\\FireableAttributeEvents\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "suggest": {
        "binary-cats/laravel-rbac": "Laravel 11 enum-backed RBAC extension of spatie/laravel-permission",
        "binary-cats/laravel-mailgun-webhooks": "Handle Mailgun webhooks in your Laravel application"
    },
    "extra": {
        "laravel": {
            "providers": [

            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
